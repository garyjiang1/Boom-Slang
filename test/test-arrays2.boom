int[] arr = [3+2, exp(5,2), 42]

print_arr3(arr)

# this function calculates b^p
def exp(int b, int p) returns int:
	if p == 0:
		return b
	int start = 1
	int i = 0
	loop i += 1 while i < p:
		start *= b
	return start

