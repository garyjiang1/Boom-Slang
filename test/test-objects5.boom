println(foo.myclass.myclass)
foo.getself(NULL).getself(NULL).printx(NULL, NULL)

println(MyClass(NULL, 0))
println(MyClass(NULL, 5000).x)
MyClass(NULL, 5001).printx(NULL, NULL)
println(MyClass(NULL, 0).to_string())

